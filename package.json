{
  "name": "fireadmin",
  "version": "0.8.8",
  "description": "Application for Managing Firebase Applications. Includes support for multiple environments and data migrations.",
  "scripts": {
    "clean": "rimraf dist",
    "build": "react-scripts build",
    "eject": "react-scripts eject",
    "start": "react-scripts start",
    "start:dist": "yarn build && firebase serve --only hosting -p 3000",
    "test": "react-scripts test",
    "build:config": "firebase-ci createConfig",
    "build:testConfig": "cypress-firebase createTestEnvFile",
    "deploy:functions": "firebase deploy --only functions",
    "deploy": "firebase-ci deploy -s",
    "docs:start": "yarn --cwd docs start",
    "docs:build": "yarn --cwd docs build",
    "functions:start": "yarn --cwd functions start",
    "functions:build": "yarn --cwd functions build",
    "functions:watch": "yarn --cwd functions watch",
    "functions:test": "yarn --cwd functions test",
    "functions:test:cov": "yarn --cwd functions test:cov",
    "lint": "eslint src functions cypress docs",
    "lint:functions": "eslint functions",
    "lint:app": "eslint src",
    "lint:tests": "eslint cypress",
    "lint:docs": "eslint docs",
    "lint:fix": "yarn lint --fix",
    "test:ui": "yarn build:testConfig && cypress run",
    "test:ui:open": "yarn build:testConfig && cypress open",
    "test:ui:stage": "yarn test:ui --env envName=stage",
    "test:ui:open:stage": "yarn test:ui:open --env envName=stage",
    "format": "prettier --single-quote --no-semi --trailing-comma none --write \"src/**/*.js\"",
    "pre-push": "yarn lint"
  },
  "dependencies": {
    "@material-ui/core": "^3.9.2",
    "@material-ui/icons": "^3.0.2",
    "classnames": "^2.2.6",
    "codemirror": "^5.32.0",
    "date-fns": "^1.29.0",
    "firebase": "^5.8.4",
    "firepad": "1.4.0",
    "history": "^4.7.2",
    "lodash": "^4.17.11",
    "prop-types": "^15.6.1",
    "react": "^16.8.3",
    "react-dnd": "^2.5.4",
    "react-dnd-html5-backend": "^2.5.4",
    "react-dom": "^16.8.3",
    "react-dropzone": "^4.2.1",
    "react-ga": "^2.3.5",
    "react-google-button": "^0.5.3",
    "react-instantsearch": "^4.2.0",
    "react-loadable": "^5.5.0",
    "react-redux": "^5.0.7",
    "react-redux-firebase": "^2.1.9",
    "react-router-dom": "^4.3.1",
    "recompose": "^0.30.0",
    "redux": "^4.0.0",
    "redux-auth-wrapper": "^2.0.3",
    "redux-firestore": "^0.6.0",
    "redux-form": "^7.4.0",
    "redux-form-material-ui": "next",
    "redux-thunk": "^2.2.0",
    "reselect": "^3.0.1"
  },
  "devDependencies": {
    "babel-eslint": "^9.0.0",
    "chai": "^4.2.0",
    "chai-as-promised": "^7.1.1",
    "chai-enzyme": "^1.0.0-beta.0",
    "cypress": "^3.4.1",
    "cypress-firebase": "^0.2.5",
    "eslint": "^5.14.0",
    "eslint-config-prettier": "^4.0.0",
    "eslint-config-react-app": "^3.0.7",
    "eslint-plugin-babel": "^5.3.0",
    "eslint-plugin-chai-friendly": "^0.4.1",
    "eslint-plugin-cypress": "^2.2.1",
    "eslint-plugin-flowtype": "^3.4.2",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-prettier": "^3.0.1",
    "eslint-plugin-react": "^7.12.4",
    "expose-loader": "^0.7.5",
    "firebase-ci": "^0.5.5",
    "firebase-tools": "^6.4.0",
    "husky": "^1.3.1",
    "jest": "^23.6.0",
    "jest-pnp-resolver": "1.0.1",
    "jest-resolve": "23.6.0",
    "node-sass": "^4.11.0",
    "prettier": "^1.15.2",
    "react-scripts": "2.1.1",
    "react-test-renderer": "^16.8.3",
    "sinon": "^5.1.1",
    "sinon-chai": "^3.1.0"
  },
  "eslintConfig": {
    "extends": "./eslintrc.js"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}"
    ],
    "resolver": "jest-pnp-resolver",
    "setupFiles": [
      "react-app-polyfill/jsdom"
    ],
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.{js,jsx}",
      "<rootDir>/src/**/?(*.)(spec|test).{js,jsx}"
    ],
    "testEnvironment": "jsdom",
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.(js|jsx)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx)$",
      "^.+\\.module\\.(css|sass|scss)$"
    ],
    "moduleNameMapper": {
      "^react-native$": "react-native-web",
      "^.+\\.module\\.(css|sass|scss)$": "identity-obj-proxy"
    },
    "moduleFileExtensions": [
      "web.js",
      "js",
      "json",
      "web.jsx",
      "jsx",
      "node"
    ],
    "snapshotResolver": "<rootDir>/scripts/snapshotResolver.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/prescottprue/fireadmin.git"
  },
  "engines": {
    "node": ">=8.11.0"
  },
  "author": "prescottprue (https://github.com/prescottprue)",
  "license": "MIT",
  "config": {
    "port": "3000"
  },
  "husky": {
    "hooks": {
      "pre-push": "yarn lint"
    }
  }
}
